{
	"name": "occ2/webloader",
	"description": "Tool for loading or deploying CSS and JS files into web pages",
	"keywords": ["webloader", "css", "javascript", "assets", "nette"],
	"homepage": "http://addons.nette.org/cs/webloader",
	"license": "MIT",
	"authors": [
		{
			"name": "Jan Marek",
			"email": "mail@janmarek.net"
		}
	],
	"autoload": {
		"psr-0": {
			"WebLoader": ""
		}
	},
	"require": {
		"nette/application": "~3.0@dev",
		"nette/di": "~3.0@dev",
		"nette/utils": "~3.0@dev"
	},
	"suggest": {
		"oyejorge/less.php": "LESS compiler written in PHP.",
		"leafo/scssphp": "SCSS compiler written in PHP.",
		"joseki/webloader-filters": "CSSMin & JSMin filters written in PHP.",
		"coffeescript/coffeescript": "CoffeeScript compiler written in PHP."
	},
	"require-dev": {
		"nette/application": "~3.0@dev",
		"nette/bootstrap": "~3.0@dev",
		"nette/caching": "~3.0@dev",
		"nette/component-model": "~3.0@dev",
		"nette/database": "~3.0@dev",
		"nette/deprecated": "~3.0@dev",
		"nette/di": "~3.0@dev",
		"nette/finder": "~2.5@dev",
		"nette/forms": "~3.0@dev",
		"nette/http": "~3.0@dev",
		"nette/mail": "~3.0@dev",
		"nette/neon": "~3.0@dev",
		"nette/php-generator": "~3.2@dev",
		"nette/reflection": "~2.4@dev",
		"nette/robot-loader": "~3.2@dev",
		"nette/safe-stream": "~2.4@dev",
		"nette/security": "~3.0@dev",
		"nette/tokenizer": "~3.0@dev",
		"nette/utils": "~3.0@dev",
		"latte/latte": "~2.5@dev",
		"tracy/tracy": "~2.6@dev",

		"oyejorge/less.php": "~1.7",
		"leafo/scssphp": "~0.7",
		"coffeescript/coffeescript": "1.3.*",

		"mockery/mockery": "1.1.*",
		"phpunit/phpunit": "8.1.*",
		"jakub-onderka/php-parallel-lint": "~1.0"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.0-dev"
		}
	}
}
